<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE = edge">
    <meta name="viewport" content="width = device-width, initial-scale = 1">

    <meta name="description" content="Nuevo Encuentro">
    <meta name="author" content="Julian Lionti - No fue magia">
    <link rel="icon" href="../../favicon.ico">

    <title>Nuevo Encuentro</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Css/bootstrap.css" rel="stylesheet">
    <link href="~/Css/iniciarsesion.css" rel="stylesheet">
    <link href="~/Css/rating.css" rel="stylesheet">
    <link href="~/Css/jquery.toast.min.css" rel="stylesheet">

    @if (ViewBag.Title == "Detalle")
    {
        <link href="~/Css/detalle.css" rel="stylesheet">
    }
    else if (ViewBag.Title == "Historial")
    {
        <link href="~/Css/historial.css" rel="stylesheet">
    }
    else if (ViewBag.Title == "Estadisticas")
    {
        <link href="~/Css/estadisticas.css" rel="stylesheet">
    }
    else
    {
        <link href="~/Css/carrito.css" rel="stylesheet">
    }


</head>
<body>
    @Html.Partial("~/Views/Compartida/_menu.cshtml")
    @RenderBody()

    <div class="container">
        <hr>
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; No Fue Magia 2016</p>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/Js/jquery-3.1.1.min.js"></script>
    <script src="~/Js/jquery-ui.js"></script>
    <script src="~/Js/bootstrap.js"></script>
    <script src="~/Js/bootstrap-table.js"></script>
    <script src="~/Js/bootstrap-table-es-ES.js"></script>
    <script src="~/Js/tano.js"></script>
    <script src="~/Js/magyp.js"></script>
    <script src="~/Js/jquery.toast.min.js"></script>
    <script src="~/Js/js.cookie.js"></script>
    <script src="~/Js/rating.js"></script>
    <script src="~/Js/menu.js"></script>
    <script src="~/Js/jquery-bootstrap-modal-steps.min.js"></script>
    <script src="~/Js/jquery.blockUI.min.js"></script>
    @if (ViewBag.Title == "Estadisticas")
    {
        <script src="~/Js/highmaps.js"></script>
    }
    @RenderSection("scripts", false)
</body>
</html>